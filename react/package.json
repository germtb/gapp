{
  "name": "@gap/react",
  "version": "0.1.0",
  "description": "React bindings for the gap full-stack framework",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@gap/client": "^0.1.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.14",
    "tsup": "^8.0.0",
    "typescript": "^5.9.3"
  },
  "license": "MIT",
  "author": "Gerard Abellan Serrano",
  "repository": {
    "type": "git",
    "url": "https://github.com/germtb/gap.git",
    "directory": "react"
  },
  "keywords": [
    "gap",
    "react",
    "hooks",
    "rpc",
    "full-stack"
  ]
}
